        <rdfs:label xml:lang="en">PO box</rdfs:label>
        <ns1:term_status xml:lang="en">testing</ns1:term_status>
    </owl:DatatypeProperty>
    


    <!-- http://www.w3.org/ns/locn#postCode -->

    <owl:DatatypeProperty rdf:about="http://www.w3.org/ns/locn#postCode">
        <rdfs:domain rdf:resource="http://www.w3.org/ns/locn#Address"/>
        <rdfs:range rdf:resource="http://www.w3.org/2000/01/rdf-schema#Literal"/>
        <terms:identifier rdf:datatype="http://www.w3.org/2001/XMLSchema#string">locn:postCode</terms:identifier>
        <rdfs:comment xml:lang="en">The post code (a.k.a postal code, zip code etc.). Post codes are common elements in many countries&apos; postal address systems. The domain of locn:postCode is locn:Address.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://www.w3.org/ns/locn"/>
        <rdfs:label xml:lang="en">post code</rdfs:label>
        <ns1:term_status xml:lang="en">testing</ns1:term_status>
    </owl:DatatypeProperty>
    


    <!-- http://www.w3.org/ns/locn#postName -->

    <owl:DatatypeProperty rdf:about="http://www.w3.org/ns/locn#postName">
